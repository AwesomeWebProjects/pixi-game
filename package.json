{
  "name": "game",
  "version": "0.0.1",
  "scripts": {
    "start": "(mkdir \"build\" || rm -rf \"build\" && mkdir \"build\" || rd /s /q \"build\" && mkdir \"build\") && node --max_old_space_size=2048 ./node_modules/rollup/dist/bin/rollup -c rollup.config.dev.js --watch",
    "start-with-sw": "(mkdir \"build\" || rm -rf \"build\" && mkdir \"build\" || rd /s /q \"build\" && mkdir \"build\") && (npm run generate-service-worker) &&  node --max_old_space_size=2048 ./node_modules/rollup/dist/bin/rollup -c rollup.config.dev.js --watch",
    "build": "(mkdir \"build\" || rm -rf \"build\" && mkdir \"build\" || rd /s /q \"build\" && mkdir \"build\") && node --max_old_space_size=2048 ./node_modules/rollup/dist/bin/rollup -c rollup.config.prod.js",
    "build:netlify": "node --max_old_space_size=2048 ./node_modules/rollup/dist/bin/rollup -c rollup.config.netlify.prod.js",
    "generate-service-worker": "node scripts/generate-service-worker.js",
    "eslint": "eslint --ext=js --ext=jsx --fix ./src",
    "stylelint": "stylelint \"src/**/*.css\"",
    "prettier": "prettier --config ./.prettierrc.json --write \"src/**/*{.js,.jsx,.css}\"",
    "prepare": "husky install"
  },
  "author": {
    "name": "Daniel Barion"
  },
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^2.4.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "postcss-url": "^10.1.3",
    "prettier": "^2.2.1",
    "rollup": "^2.45.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-browsersync": "^1.3.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-html-scaffold": "^0.2.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-stylus-to-css": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "stylelint": "^13.7.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0"
  },
  "dependencies": {
    "@pixi/loaders": "^6.0.2",
    "@pixi/tilemap": "^3.1.0",
    "i18next": "^20.2.1",
    "redux": "^4.0.5",
    "pixi.js": "^6.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
